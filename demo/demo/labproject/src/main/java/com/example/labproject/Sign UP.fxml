<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="parentPane" prefHeight="682.0" prefWidth="600.0" style="-fx-background-color: #23766e;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labproject.SignUP">
   <children>
      <TextField fx:id="Email" layoutX="114.0" layoutY="209.0" onAction="#mail" prefHeight="26.0" prefWidth="370.0" promptText="Enter Your Email:" />
      <TextField fx:id="usname" layoutX="114.0" layoutY="362.0" onAction="#UserName" prefHeight="26.0" prefWidth="370.0" promptText="Enter Username:" />
      <PasswordField fx:id="pass" layoutX="114.0" layoutY="438.0" onAction="#Password" prefHeight="26.0" prefWidth="370.0" promptText="Create Password:" />
      <Button fx:id="create" alignment="CENTER" layoutX="116.0" layoutY="514.0" mnemonicParsing="false" onAction="#CreateID" style="-fx-background-color: #FFFFFF;" text="Create Your Account" textFill="#23766e" />
      <Button fx:id="loginPG" alignment="CENTER" layoutX="116.0" layoutY="578.0" mnemonicParsing="false" onAction="#GoToLogin" prefHeight="30.0" prefWidth="81.0" style="-fx-background-color: #FFFFFF;" text="Login Page" textFill="#23766e" />
      <RadioButton fx:id="male" layoutX="116.0" layoutY="287.0" mnemonicParsing="false" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="female" layoutX="213.0" layoutY="287.0" mnemonicParsing="false" text="Female" toggleGroup="$gender" />
      <RadioButton fx:id="otherG" layoutX="316.0" layoutY="287.0" mnemonicParsing="false" text="Other" toggleGroup="$gender" />
      <ImageView fx:id="process" fitHeight="150.0" fitWidth="200.0" layoutX="384.0" layoutY="518.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="193.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../resources/com/example/labproject/Project%20Alo%20PNG%20Logo%20White.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
